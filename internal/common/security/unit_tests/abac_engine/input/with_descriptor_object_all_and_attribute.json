{
  "AllAccessPermissionRules": {
    "rules": [
      {
        "ACL": {
          "ACCESS": "ALLOW",
          "RIGHTS": ["READ","UPDATE"],
          "ATTRIBUTES": [{"CLAIM": "role"}]
        },
        "OBJECTS": [{"ROUTE": "/shell-descriptors"}, {"ROUTE": "/shell-descriptors/*"}, {"DESCRIPTOR" : "$aasdesc(\"*\")"}],
        "FORMULA": { "$eq": [ 
          { "$attribute": { "CLAIM": "role" } }, 
          { "$strVal": "admin" } 
          ] 
        }
      }
    ]
  }
}
