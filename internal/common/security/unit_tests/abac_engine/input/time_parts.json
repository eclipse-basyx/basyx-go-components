{
  "AllAccessPermissionRules": {
    "rules": [
      {
        "ACL": {
          "ACCESS": "ALLOW",
          "RIGHTS": ["READ"],
          "ATTRIBUTES": [{"GLOBAL": "ANONYMOUS"}]
        },
        "OBJECTS": [{"ROUTE": "/shell-descriptors"}],
        "FORMULA": {
          "$and": [
            {
              "$starts-with": [
                {"$strCast": {"$attribute": {"GLOBAL": "UTCNOW"}}},
                {"$strVal": "2025-01-01"}
              ]
            },
            {
              "$regex": [
                {"$strCast": {"$attribute": {"GLOBAL": "UTCNOW"}}},
                {"$strVal": "^2025-01-01T00"}
              ]
            },
            {
              "$contains": [
                {"$strCast": {"$attribute": {"GLOBAL": "UTCNOW"}}},
                {"$strVal": "2025-01-01"}
              ]
            },
            {
              "$eq": [
                {"$timeCast": {"$attribute": {"GLOBAL": "LOCALNOW"}}},
                {"$timeVal": "01:00:00"}
              ]
            }
          ]
        }
      }
    ]
  }
}
