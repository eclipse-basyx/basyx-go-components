{
  "AllAccessPermissionRules": {
    "rules": [
      {
        "ACL": {
          "ACCESS": "ALLOW",
          "RIGHTS": ["READ","UPDATE"],
          "ATTRIBUTES": [ { "CLAIM": "role" }]
        },
        "OBJECTS": [{"ROUTE" : "/*"}],
        "FORMULA": { "$starts-with": [ { "$attribute": { "CLAIM": "role" } }, { "$strVal": "view" } ] }
      }
    ]
  }
}