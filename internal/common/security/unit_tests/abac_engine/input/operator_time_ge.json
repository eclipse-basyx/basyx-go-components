{
  "AllAccessPermissionRules": {
    "rules": [
      {
        "ACL": {
          "ACCESS": "ALLOW",
          "RIGHTS": ["READ"],
          "ATTRIBUTES": [{"GLOBAL": "ANONYMOUS"}]
        },
        "OBJECTS": [
          {
            "ROUTE": "/shell-descriptors"
          }
        ],
        "FORMULA": {
          "$ge": [
            {
              "$timeCast": {
                "$attribute": {
                  "GLOBAL": "LOCALNOW"
                }
              }
            },
            {
              "$timeCast": {
                "$strVal": "01:00:00"
              }
            }
          ]
        }
      }
    ]
  }
}
