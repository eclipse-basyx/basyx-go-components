{
  "AllAccessPermissionRules": {
    "rules": [
      {
        "ACL": {
          "ACCESS": "ALLOW",
          "RIGHTS": [
            "READ"
          ],
          "ATTRIBUTES": [{"CLAIM": "role"}]
        },
        "OBJECTS": [
          {
            "ROUTE": "/*"
          }
        ],
        "FORMULA": { "$regex": [ { "$attribute": { "CLAIM": "role" } }, { "$strVal": "^test-[0-9]+$" } ] }
      }
    ]
  }
}
